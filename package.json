{
	"name": "dropzone",
	"version": "0.2.1",
	"private": true,
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"scripts": {
		"dev": "dotenv -e .env.local -- next dev",
		"bot": "dotenv -e .env.local -- tsx ./bot/index.ts",
		"build": "dotenv -e .env.production -- next build",
		"start": "dotenv -e .env.production -- next start",
		"dev:bot": "dotenv -e .env -- tsx --watch bot/index.ts",
		"build:bot": "dotenv -e .env.production -- tsc -p tsconfig.bot.json && tsc-alias -p tsconfig.bot.json",
		"start:bot": "dotenv -e .env.production -- node dist/bot/bot/index.js",
		"dev:https": "dotenv -e .env.local NODE_TLS_REJECT_UNAUTHORIZED=0 next dev -p 3443 --https --ssl-cert ./localhost.pem --ssl-key ./localhost-key.pem"
	},
	"dependencies": {
		"@floating-ui/react": "^0.27.13",
		"@grammyjs/conversations": "^2.1.0",
		"@prisma/client": "^6.11.1",
		"@radix-ui/react-accordion": "^1.2.11",
		"@sendgrid/mail": "^8.1.6",
		"@splidejs/react-splide": "^0.7.12",
		"@telegram-apps/init-data-node": "^2.0.10",
		"@telegram-apps/sdk-react": "^3.3.6",
		"@twa-dev/sdk": "^8.0.2",
		"@types/chalk": "^0.4.31",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/nanoid": "^2.1.0",
		"@types/nodemailer": "^7.0.3",
		"@types/qs": "^6.14.0",
		"@types/react-countup": "^4.3.0",
		"@uiball/loaders": "^1.3.1",
		"axios": "^1.10.0",
		"body-parser": "^2.2.0",
		"callback-data": "^1.1.1",
		"chalk": "^5.6.0",
		"cheerio": "^1.1.2",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"framer-motion": "^12.23.12",
		"grammy": "^1.38.2",
		"jsonwebtoken": "^9.0.2",
		"lucide-react": "^0.525.0",
		"motion": "^12.23.3",
		"nanoid": "^5.1.5",
		"next": "^15.5.2",
		"nodemailer": "^7.0.10",
		"openai": "^5.10.2",
		"qs": "^6.14.0",
		"react": "^19.0.0",
		"react-countup": "^6.5.3",
		"react-dom": "^19.0.0",
		"react-hot-toast": "^2.6.0",
		"react-intersection-observer": "^9.16.0",
		"react-loading-skeleton": "^3.5.0",
		"react-scroll": "^1.9.3",
		"react-splide-ts": "^0.7.14",
		"swiper": "^11.2.10",
		"swr": "^2.3.4",
		"telegraf": "^4.16.3",
		"zod": "^3.25.76",
		"zustand": "^5.0.6"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"cssnano": "^7.0.7",
		"dotenv-cli": "^9.0.0",
		"eslint": "^9",
		"eslint-config-next": "15.3.5",
		"prisma": "^6.11.1",
